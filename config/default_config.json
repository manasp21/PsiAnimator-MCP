{
  "quantum": {
    "precision": 1e-12,
    "max_hilbert_dimension": 1024,
    "enable_gpu": false
  },
  "animation": {
    "quality": "medium_quality",
    "frame_rate": 30,
    "output_format": "mp4",
    "cache_animations": true,
    "cache_size_mb": 500
  },
  "server": {
    "log_level": "INFO",
    "enable_logging": true,
    "output_directory": "/app/output"
  },
  "manim": {
    "renderer": "cairo",
    "background_color": "#000000",
    "tex_template": "default"
  }
}